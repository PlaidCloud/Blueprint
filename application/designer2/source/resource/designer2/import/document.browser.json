                            ".filter(and_(", 
                            "DocumentAccounts.PlaidGroup == u'%(PLAID_GROUP)s',", 
                            "DocumentAccounts.AccountID == u'%(account)s',", 
                            "or_(DocumentAccounts.IsPublic == 1, DocumentAccounts.OwnerID == u'%(USER_ID)s')", 
